{
  "name": "sk-testing",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 concurrently \"pnpm run vite-dev\" \"pnpm i18n\"",
    "vite-dev": "vite dev --mode development",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write --plugin prettier-plugin-svelte .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "i18n": "typesafe-i18n",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.8",
    "@playwright/test": "^1.39.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/svelte": "^7.5.3",
    "@storybook/sveltekit": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.27.2",
    "@types/cookie": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@unocss/extractor-svelte": "^0.57.1",
    "@unocss/preset-icons": "^0.57.1",
    "@unocss/preset-mini": "^0.57.1",
    "@unocss/transformer-variant-group": "^0.57.1",
    "@vite-pwa/sveltekit": "^0.2.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.34.1",
    "lightningcss": "^1.22.0",
    "mdsvex": "^0.11.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unocss": "^0.57.1",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@panva/hkdf": "^1.1.1",
    "@unocss/reset": "^0.57.1",
    "bits-ui": "^0.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "eslint-plugin-prettier": "^5.0.1",
    "formsnap": "^0.4.0",
    "jose": "^5.0.2",
    "lucide-svelte": "^0.292.0",
    "openid-client": "^5.6.1",
    "sveltekit-superforms": "^1.9.0",
    "tailwind-merge": "^2.0.0",
    "tailwind-variants": "^0.1.18",
    "typesafe-i18n": "^5.26.2",
    "zod": "^3.22.4"
  }
}
